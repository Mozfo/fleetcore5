{
  "name": "fleetcore5",
  "packageManager": "pnpm@10.18.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "lsof -ti:3000 | xargs kill -9 || true",
    "dev": "FORCE_COLOR=true next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm typecheck && pnpm lint && pnpm format:check",
    "validate:fix": "pnpm typecheck && pnpm lint:fix && pnpm format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "seed:crm-settings": "dotenv -e .env.local -- tsx scripts/seed-crm-settings.ts",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "prisma:pull": "dotenv -e .env.local -- prisma db pull",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run lib/core/__tests__/*.test.ts",
    "test:integration": "DATABASE_URL=file:./test-integration.db vitest run --config vitest.config.integration.ts",
    "test:core": "pnpm test:unit && pnpm test:integration",
    "test:phase0.2": "vitest run lib/validators lib/middleware",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "validate:sql": "dotenv -e .env.local -- tsx scripts/validate-sql.ts",
    "test:critical": "vitest run __tests__/critical-paths/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install chromium"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
  },
