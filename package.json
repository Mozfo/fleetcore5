{
  "name": "fleetcore5",
  "packageManager": "pnpm@10.18.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "lsof -ti:3000 | xargs kill -9 || true",
    "dev": "FORCE_COLOR=true next dev --turbo 2>&1 | pino-pretty -t -c",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm typecheck && pnpm lint && pnpm format:check",
    "validate:fix": "pnpm typecheck && pnpm lint:fix && pnpm format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "prisma:pull": "dotenv -e .env.local -- prisma db pull",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run lib/core/__tests__/*.test.ts",
    "test:integration": "DATABASE_URL=file:./test-integration.db vitest run --config vitest.config.integration.ts",
    "test:core": "pnpm test:unit && pnpm test:integration",
    "test:phase0.2": "vitest run lib/validators lib/middleware",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:audit": "dotenv -e .env.local -- tsx scripts/test-audit-manual.ts",
    "test:audit:e2e": "dotenv -e .env.local -- tsx scripts/test-audit-e2e.ts",
    "validate:sql": "dotenv -e .env.local -- tsx scripts/validate-sql.ts",
    "test:batch3": "dotenv -e .env.test -- tsx scripts/test-batch3-staging.ts",
    "test:batch3:verbose": "dotenv -e .env.test -- tsx scripts/test-batch3-staging.ts --verbose",
    "test:batch3:ci": "tsx scripts/test-batch3-staging.ts"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.2",
    "@formkit/auto-animate": "^0.9.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "6.18.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^1.0.0",
    "@react-email/render": "^1.3.2",
    "@sentry/nextjs": "^10.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.19",
    "i18next": "^25.5.2",
    "i18next-resources-to-backend": "^1.2.1",
    "jose": "^5.10.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^16.0.0",
    "react-intersection-observer": "^9.16.0",
    "resend": "^6.1.0",
    "sonner": "^2.0.7",
    "svix": "^1.76.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@clerk/backend": "^2.18.3",
    "@eslint/eslintrc": "^3.3.1",
    "@react-email/preview-server": "^5.0.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testcontainers/postgresql": "^11.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "6.18.0",
    "react-email": "^5.0.1",
    "tailwindcss": "^4.1.13",
    "testcontainers": "^11.8.0",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
